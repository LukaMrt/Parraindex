{% extends '/layouts/default.twig' %}

{% set style = style('tree.css') %}
{% set title = 'Parraindex – Arbre IUT Lyon 1' %}

{% block javascripts %}
    <script src='{{ script("carousel.js") }}' defer></script>
{% endblock %}

{% block metadatas %}
