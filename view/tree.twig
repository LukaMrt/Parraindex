    <meta name="keywords" content="Parraindex, arbre, généalogie">

    <meta property="og:type" content="website">
    <meta property="og:image" content="{{ icon('logo-blue.svg') }}">
{% endblock %}

{% block content %}

    {% include('components/carousel.twig') %}
    {% include('components/carousel-controller.twig') %}

{% endblock %}